  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "MathGame (attach)",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector",
  },
  {
      "type": "node",
      "request": "launch",
      "name": "MathGame (launch)",
      "program": "${workspaceFolder}/api/dist/app.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}